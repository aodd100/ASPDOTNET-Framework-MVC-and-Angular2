{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es2015", "dom" ],
    "noImplicitAny": false,
    "noEmitOnError": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "../wwwroot/app",
    "removeComments": true
  }
}
